# ===========================================
# CONFIGURACIÓN DEL AI ENGINE
# ===========================================
# Copia este archivo a .env y completa los valores

# --- Aplicación ---
APP_NAME=AI-Engine
APP_ENV=development
DEBUG=true
SECRET_KEY=tu-clave-secreta-muy-larga-y-segura-cambiar-en-produccion

# --- Base de Datos PostgreSQL ---
DATABASE_URL=postgresql+asyncpg://aiengine:devpassword123@localhost:5432/aiengine
# Para producción usar:
# DATABASE_URL=postgresql+asyncpg://usuario:password@host:5432/database

# --- Qdrant Vector Database ---
QDRANT_HOST=localhost
QDRANT_PORT=6333
# Para Qdrant Cloud:
# QDRANT_URL=https://xxx.qdrant.io
# QDRANT_API_KEY=tu-api-key

# --- OpenAI ---
OPENAI_API_KEY=sk-tu-api-key-de-openai
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# --- Twilio (WhatsApp y Voice) ---
TWILIO_ACCOUNT_SID=tu-account-sid
TWILIO_AUTH_TOKEN=tu-auth-token
TWILIO_WHATSAPP_NUMBER=+14155238886
TWILIO_PHONE_NUMBER=+1234567890

# --- Meta/Facebook (Messenger) ---
META_APP_ID=tu-app-id
META_APP_SECRET=tu-app-secret
META_PAGE_ACCESS_TOKEN=tu-page-access-token
META_VERIFY_TOKEN=tu-token-de-verificacion-webhook

# --- ElevenLabs (Text to Speech) ---
ELEVENLABS_API_KEY=tu-api-key-elevenlabs
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# --- CORS ---
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# --- Logging ---
LOG_LEVEL=INFO
